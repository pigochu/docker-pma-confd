    "compilerOptions": {
        "target": "ES2021",
        "lib": ["ES2015","ES2016","ES2017","DOM"],
        "module": "CommonJS",
        "rootDir": "./src", 
        "outDir": "./dist",
        "strict": true,
        "skipLibCheck": true,
        "esModuleInterop": true
